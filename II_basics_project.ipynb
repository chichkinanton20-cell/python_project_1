    "          enemy_atk_fight:int,\n",
    "          enemy_def_fight:int,\n",
    "          verbose:bool):\n",
    "    \n",
    "    if verbose:print(\"햏햟혢햦햫햟햣혝혜혪 햠쮏!\")\n",
    "\n",
    "    while True:\n",
    "        \n",
    "       #혞햢햟 햣혜쮏쫧썛웷\n",
    "\n",
    "       enemy_hp_fight -= calculate_damage(character_atk, enemy_def_fight)\n",
    "       if verbose: print(f\"햣혜쮏쫧썛 햠혧혬혝 혜 햟혝햟햨쮏 {character_atk} 쮐햦 향햟혤햦혝혦 {enemy_def_fight}, 햫햟햫쮐햦혝 {calculate_damage(character_atk, enemy_def_fight)} 혞쮏쫧\")\n",
    "       if enemy_hp_fight == 0 or enemy_hp_fight < 0:\n",
    "           if verbose: print(f\"쮏쐃왏얧! 햒 햣혜쮏쫧썛웷 쮐혝햟햩쮐혧 {character_hp}\")\n",
    "           return character_hp\n",
    "           break\n",
    "       \n",
    "       #혞햢햟 쮐햦쒫쫧쟳쥃\n",
    "\n",
    "       character_hp -= calculate_damage(enemy_atk_fight, character_def)\n",
    "       if verbose: print(f\"쮐햦쒫쫧쟳 햠혧혬혝 혜 햟혝햟햨쮏 {enemy_atk_fight} 쮐햦 향햟혤햦혝혦 {character_def}, 햫햟햫쮐햦혝 {calculate_damage(enemy_atk_fight, character_def)} 혞쮏쫧\")\n",
    "       if character_hp == 0 or character_hp < 0:\n",
    "           if verbose: print(\"쮐햟햤햣햫햦햣! 햊햡햟 쮏쥃쮏쫨햣햫햟!\")\n",
    "           return 0\n",
    "           break\n",
    "       \n",
